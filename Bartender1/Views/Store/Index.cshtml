@model IEnumerable<Bartender1.Models.Genre>

@{
    ViewBag.Title = "Store";
}

<h3>Browse Drinks</h3>
<p>
    Select from @Model.Count()
    genres:
</p>

<ul>
    @foreach (var genre in Model)
    {
        <li>
            @Html.ActionLink(genre.Name,"Browse", new { genre = genre.Name })
        </li>
    }
</ul>
